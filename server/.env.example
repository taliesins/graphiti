OPENAI_API_KEY=

NEO4J_PORT=7687 # This seems specific to a docker-compose setup, might not be directly used by Graphiti with provider model. Keeping for existing context.

# Graph Database Provider Configuration
GRAPH_DB_PROVIDER=neo4j # Options: "neo4j", "kuzudb"

# Neo4j Configuration (only needed if GRAPH_DB_PROVIDER is "neo4j")
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# KuzuDB Configuration (only needed if GRAPH_DB_PROVIDER is "kuzudb")
# KUZUDB_DATABASE_PATH=./data/kuzu_db # Example path, ensure directory exists or Kuzu can create it.
# KUZUDB_IN_MEMORY=False # Set to True to run KuzuDB in-memory (DATABASE_PATH might be ignored or used differently)